{
  "name": "nomadeswebsite",
  "version": "1.0.0",
  "description": "Un sitio web presentacional creado con express.js.",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "concurrently \"cross-env NODE_ENV=development nodemon index.js\" \"parcel src/public/main.js src/public/styles.css --dist-dir dist --opende\"",
    "build": "cross-env NODE_ENV=production parcel build src/public/main.js src/public/styles.css --dist-dir dist  --no-source-maps --no-content-hash",
    "preview": "cross-env NODE_ENV=preview nodemon index.js",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "test:cov": "cross-env NODE_ENV=test jest --detectOpenHandles --coverage"
  },
  "author": "Antorrg",
  "license": "ISC",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.3",
    "cloudinary": "^2.5.0",
    "connect-session-sequelize": "^7.1.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.4.4",
    "pug": "^3.0.3",
    "sequelize": "^6.37.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@parcel/optimizer-cssnano": "^2.12.0",
    "@parcel/optimizer-terser": "^2.12.0",
    "@parcel/transformer-pug": "^2.12.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "concurrently": "^9.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "parcel": "^2.12.0",
    "supertest": "^7.0.0"
  },
  "babel": {
    "env": {
      "test": {
        "presets": ["@babel/preset-env"],
        "plugins": ["babel-plugin-transform-import-meta"]
      }
    }
  }

}
